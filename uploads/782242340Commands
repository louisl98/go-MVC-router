sudo chown -R louis '/path' /* admin privileges */

sudo fuser -k -n tcp 3001 /* kill process port 3001 */

echo fs.inotify.max_user_watches=524288 | sudo tee -a /etc/sysctl.conf && sudo sysctl -p /* nodemon max watchers */

git rm --cached filename /* cache .gitignore */

git rm -r --cached . /* full cache clear */


sudo apt-get install gcc-6 g++-6
CC=$(which gcc-6) rbenv install 2.2.1 /* ruby specific version (2.2.1) for ubuntu */

nautilus '/path' /* open in file explorer */

